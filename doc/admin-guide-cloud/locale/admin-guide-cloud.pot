msgstr ""

#: ./doc/admin-guide-cloud/ch_compute.xml:80(link)
msgid "VMWare vSphere"
msgstr ""

#: ./doc/admin-guide-cloud/ch_compute.xml:87(link)
msgid "Xen"
msgstr ""

