msgid "projectName"
msgstr "projectName"

msgid "https://identityHost:portNumber/v2.0"
msgstr "https://identityHost:portNumber/v2.0"

msgid "tenantIDString"
msgstr "tenantIDString"

msgid "regionName"
msgstr "regionName"

