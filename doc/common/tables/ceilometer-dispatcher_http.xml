  <table rules="all" xml:id="config_table_ceilometer_dispatcher_http">
    <caption>Description of HTTP dispatcher configuration options</caption>
    <col width="50%"/>
    <col width="50%"/>
    <thead>
      <tr>
        <th>Configuration option = Default value</th>
        <th>Description</th>
      </tr>
