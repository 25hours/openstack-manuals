  <table rules="all" xml:id="config_table_keystone_kvs">
    <caption>Description of KVS configuration options</caption>
    <col width="50%"/>
    <col width="50%"/>
    <thead>
      <tr>
        <th>Configuration option = Default value</th>
        <th>Description</th>
      </tr>
