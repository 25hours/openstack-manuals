msgstr ""

#: ./doc/glossary/glossary-terms.xml4240(para)
msgid "An open source SQL toolkit for Python, used in OpenStack."
msgstr ""

#: ./doc/glossary/glossary-terms.xml4245(glossterm)
msgid "SQLite"
msgstr ""

#: ./doc/glossary/glossary-terms.xml4247(para)
msgid ""
