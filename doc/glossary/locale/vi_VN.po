msgstr "Open vSwitch"

msgid "Open vSwitch (OVS) agent"
msgstr "Open vSwitch (OVS) agent"

msgid "Open vSwitch neutron plug-in"
msgstr "Open vSwitch neutron plug-in"

msgid "OpenLDAP"
msgstr "OpenLDAP"

